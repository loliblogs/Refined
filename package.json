{
  "name": "refined",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "node -e \"try{fs.copyFileSync('.env.example','.env',1)}catch{}\" && astro db push --remote",
    "dev": "astro dev --remote",
    "dev:full": "astro dev --remote --force",
    "build": "astro build --remote",
    "build:full": "astro build --force --remote",
    "preview": "astro preview",
    "sync": "astro sync",
    "typecheck": "astro check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:interactive": "eslint-interactive .",
    "online": "pnpm run online:workers && pnpm run online:pages && pnpm run online:vercel && pnpm run online:netlify",
    "online:workers": "pnpx wrangler@latest -c wrangler-workers.jsonc deploy",
    "online:pages": "pnpx wrangler@latest pages deploy",
    "online:vercel": "pnpx vercel@latest --cwd dist deploy --prod",
    "online:netlify": "pnpx --package netlify-cli@latest netlify deploy --no-build --prod"
  },
  "dependencies": {
