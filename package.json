{
  "name": "refined",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "node -e \"try{fs.copyFileSync('.env.example','.env',1)}catch{}\" && astro db push --remote",
    "dev": "astro dev --remote",
    "dev:full": "astro dev --remote --force",
    "build": "astro build --remote && pagefind && pagefind --site dist/oi",
    "build:full": "astro build --force && pagefind && pagefind --site dist/oi",
    "preview": "astro preview",
    "sync": "astro sync",
    "typecheck": "astro check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:interactive": "eslint-interactive .",
    "online": "pnpm run online:workers && pnpm run online:pages && pnpm run online:vercel && pnpm run online:netlify",
    "online:workers": "pnpx wrangler -c wrangler-workers.json deploy",
    "online:pages": "pnpx wrangler pages deploy",
    "online:vercel": "pnpx vercel --cwd dist deploy --prod",
    "online:netlify": "pnpx --package netlify-cli netlify deploy --no-build --prod"
  },
  "dependencies": {
    "@astrojs/db": "^0.18.0",
    "@astrojs/markdown-remark": "^6.3.8",
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/preact": "^4.1.1",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/ts-plugin": "^1.10.5",
    "@babel/preset-typescript": "^7.27.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.3",
    "@expressive-code/plugin-line-numbers": "^0.41.3",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@fontsource-variable/noto-serif-sc": "^5.2.9",
    "@mathjax/mathjax-newcm-font": "^4.0.0",
    "@mathjax/src": "^4.0.0",
    "@pagefind/default-ui": "^1.4.0",
    "@phosphor-icons/core": "^2.1.1",
    "@phosphor-icons/react": "^2.1.10",
    "@playform/compress": "^0.2.0",
    "@preact/compat": "^18.3.1",
    "@tailwindcss/vite": "^4.1.14",
    "argon2": "^0.44.0",
    "argon2-browser": "^1.18.0",
    "astro": "^5.14.5",
    "astro-expressive-code": "^0.41.3",
    "cheerio": "^1.1.2",
    "chroma-js": "^3.1.2",
    "clsx": "^2.1.1",
    "giscus": "^1.6.0",
    "hast-util-to-text": "^4.0.2",
    "hastscript": "^9.0.1",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-math": "^3.0.0",
    "medium-zoom-next": "^2.0.1",
    "micromark-extension-math": "^3.1.0",
    "overlayscrollbars": "^2.12.0",
    "pagefind": "^1.4.0",
    "preact": "^10.27.2",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "rehype": "^13.0.2",
    "remark": "^15.0.1",
    "remark-directive": "^4.0.0",
    "remark-emoji": "^5.0.2",
    "remark-github-admonitions-to-directives": "^2.1.0",
    "remark-pangu": "^2.2.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "vite": "^7.1.10",
    "wordcloud": "^1.2.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/css": "^0.13.0",
    "@eslint/js": "^9.37.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/argon2-browser": "^1.18.4",
    "@types/chroma-js": "^3.1.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/hast": "^3.0.4",
    "@types/mathjax": "^0.0.40",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/wordcloud": "^1.2.2",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-interactive": "^12.0.0",
    "eslint-mdx": "^3.6.2",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-better-tailwindcss": "^3.7.10",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-react-refresh": "^0.4.24",
    "mdast-util-directive": "^3.1.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "typescript-eslint": "^8.46.1",
    "vfile": "^6.0.3"
  },
  "pnpm": {
    "overrides": {
      "html-minifier-terser": "npm:html-minifier-next",
      "vite": "^7.1.10"
    },
    "onlyBuiltDependencies": [
      "argon2",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-frontmatter",
      "remark-math",
      "remark-github-admonitions-to-directives",
      "remark-directive",
      "remark-emoji",
      "remark-gfm"
    ]
  },
  "packageManager": "pnpm@10.18.3"
}
